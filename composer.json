{
    "name": "mckenziearts/blade-untitledui-icons",
    "description": "A package to easily make use of UntitledUI icons in your Laravel Blade views.",
    "keywords": ["Blade", "Untitled UI", "Laravel"],
    "homepage": "https://github.com/mckenziearts/blade-untitledui-icons",
    "license": "MIT",
    "funding": [
        {
            "type": "custom",
            "url": "https://laravel.cm/sponsors"
        }
    ],
    "authors": [
        {
            "name": "Arthur Monney",
            "homepage": "https://arthurmonney.me"
        }
    ],
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-icons": "^1.4",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mckenziearts\\BladeUntitledUIIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mckenziearts\\BladeUntitledUIIcons\\BladeUntitledUIIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
